{
  "display_name": "LinkedIn",
  "domains": [
    "linkedin.com"
  ],
  "page_type": {
    "content": {
      "actions": [
        {
          "intent": "like",
          "selector": "button[data-control-name='like_toggle']",
          "type": "click"
        },
        {
          "intent": "comment",
          "selector": "button[data-control-name='comment_action']",
          "type": "click"
        },
        {
          "intent": "share",
          "selector": "button[data-control-name='share_dialog']",
          "type": "click"
        },
        {
          "intent": "send",
          "selector": "button[data-control-name='send_share']",
          "type": "click"
        }
      ],
      "container_selector": "div.feed-shared-update-v2",
      "content_type": "post",
      "fields": [
        {
          "extraction": "text",
          "name": "author_name",
          "selector": "span.feed-shared-actor__name"
        },
        {
          "extraction": "text",
          "name": "content",
          "selector": ".feed-shared-update-v2__content"
        },
        {
          "attribute": "datetime",
          "extraction": "attribute",
          "name": "timestamp",
          "selector": "time"
        },
        {
          "extraction": "text",
          "name": "like_count",
          "selector": "span.social-details-social-counts__reactions-count",
          "transform": "parse_number"
        },
        {
          "extraction": "text",
          "name": "comment_count",
          "selector": "span.social-details-social-counts__comments",
          "transform": "parse_number"
        }
      ],
      "is_list": true
    },
    "display_name": "Main Feed",
    "page_type_id": "feed",
    "root_selector": "main",
    "url_patterns": [
      "/",
      "/feed"
    ]
  },
  "platform_id": "linkedin"
}