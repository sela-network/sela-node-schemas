{
  "display_name": "X Platform",
  "domains": [
    "x.com",
    "www.x.com",
    "m.x.com"
  ],
  "page_type": {
    "content": {
      "actions": [
        {
          "intent": "like",
          "risk_level": 2,
          "selector": {
            "fallbacks": [
              "[aria-label*='like']",
              "[data-testid*='like']"
            ],
            "primary": "button[aria-label*='Like']"
          },
          "type": "click"
        },
        {
          "intent": "share",
          "risk_level": 3,
          "selector": {
            "fallbacks": [
              "[aria-label*='share']",
              "[data-testid*='share']"
            ],
            "primary": "button[aria-label*='Share']"
          },
          "type": "click"
        },
        {
          "intent": "comment",
          "risk_level": 4,
          "selector": {
            "fallbacks": [
              "[aria-label*='reply']"
            ],
            "primary": "button[aria-label*='Comment']"
          },
          "type": "click"
        }
      ],
      "container_selector": {
        "fallbacks": [
          "[role='article']",
          "[role='listitem']",
          "[data-urn]"
        ],
        "primary": "[data-testid='post']"
      },
      "content_type": "post",
      "fields": [
        {
          "extraction": "text",
          "field_type": "text",
          "name": "title",
          "selector": {
            "fallbacks": [
              "h2",
              "h3",
              "a[href*='/status'] span"
            ],
            "primary": "[role='heading']"
          }
        },
        {
          "extraction": {
            "attribute": "href"
          },
          "field_type": "url",
          "name": "link",
          "selector": {
            "fallbacks": [
              "a[href*='/status']",
              "a[href]:first-of-type"
            ],
            "primary": "h2 a, h3 a"
          }
        },
        {
          "extraction": "text",
          "field_type": "text",
          "name": "subtitle",
          "selector": {
            "fallbacks": [
              "p",
              "[data-testid='description']"
            ],
            "primary": "[class*='description']"
          }
        },
        {
          "extraction": {
            "attribute": "src"
          },
          "field_type": "image",
          "name": "image",
          "selector": {
            "fallbacks": [
              "picture img",
              "img[width]",
              "img"
            ],
            "primary": "img:not([class*='avatar']):not([class*='Avatar'])"
          }
        },
        {
          "extraction": {
            "attribute": "datetime"
          },
          "field_type": "timestamp",
          "name": "timestamp",
          "selector": {
            "fallbacks": [
              "[data-time]",
              "[datetime]",
              "time"
            ],
            "primary": "time[datetime]"
          }
        },
        {
          "default": 0,
          "extraction": "text",
          "field_type": "number",
          "name": "engagement_count",
          "selector": {
            "fallbacks": [
              "button[aria-label*='like'] span",
              "[data-testid*='like'] span"
            ],
            "primary": "[aria-label*='reactions'] span"
          },
          "transform": "parse_compact_number"
        }
      ],
      "is_list": true,
      "semantic_role": "article"
    },
    "display_name": "Search Feed",
    "page_type_id": "search_feed",
    "root_selector": null,
    "url_patterns": [
      "/search",
      "/search*"
    ]
  },
  "platform_id": "x_com"
}