{
  "display_name": "LinkedIn",
  "domains": [
    "linkedin.com",
    "www.linkedin.com"
  ],
  "page_type": {
    "content": {
      "actions": [
        {
          "intent": "like",
          "risk_level": 2,
          "selector": {
            "fallbacks": [
              "[aria-label*='like']",
              "[data-testid*='like']"
            ],
            "primary": "button[aria-label*='Like']"
          },
          "type": "click"
        },
        {
          "intent": "share",
          "risk_level": 3,
          "selector": {
            "fallbacks": [
              "[aria-label*='share']",
              "[data-testid*='share']"
            ],
            "primary": "button[aria-label*='Share']"
          },
          "type": "click"
        },
        {
          "intent": "comment",
          "risk_level": 4,
          "selector": {
            "fallbacks": [
              "[aria-label*='comment']",
              "[aria-label*='reply']"
            ],
            "primary": "button[aria-label*='Comment']"
          },
          "type": "click"
        }
      ],
      "container_selector": {
        "fallbacks": [
          "div[data-urn]",
          "article[role='article']"
        ],
        "primary": "[role='listitem']"
      },
      "content_type": "result_item",
      "fields": [
        {
          "extraction": "text",
          "field_type": "text",
          "name": "title",
          "selector": {
            "fallbacks": [
              "h3 a",
              "div[role='heading'] a"
            ],
            "primary": "a span[aria-hidden='true']"
          }
        },
        {
          "extraction": {
            "attribute": "href"
          },
          "field_type": "url",
          "name": "link",
          "selector": {
            "fallbacks": [
              "a[href*='/in/']",
              "a[href*='/profile/']"
            ],
            "primary": "a[data-test-app-aware-link]"
          }
        },
        {
          "extraction": "text",
          "field_type": "text",
          "name": "subtitle",
          "selector": {
            "fallbacks": [
              "[aria-hidden='true']:nth-of-type(2)",
              "p span[dir='ltr']"
            ],
            "primary": "div:nth-of-type(2) span[dir='ltr']"
          }
        },
        {
          "extraction": {
            "attribute": "src"
          },
          "field_type": "image",
          "name": "image",
          "selector": {
            "fallbacks": [
              "img[class*='Avatar']",
              "img[class*='Entity']",
              "img"
            ],
            "primary": "img[class*='Photo']"
          }
        },
        {
          "extraction": {
            "attribute": "datetime"
          },
          "field_type": "timestamp",
          "name": "timestamp",
          "selector": {
            "fallbacks": [
              "[data-time]",
              "[datetime]",
              "time"
            ],
            "primary": "time[datetime]"
          }
        },
        {
          "default": 0,
          "extraction": "text",
          "field_type": "number",
          "name": "engagement_count",
          "selector": {
            "fallbacks": [
              "[aria-label*='like'] span",
              "button span"
            ],
            "primary": "[aria-label*='reaction'] span"
          },
          "transform": "parse_compact_number"
        }
      ],
      "is_list": true,
      "semantic_role": "listitem"
    },
    "display_name": "Search Results",
    "page_type_id": "search_results",
    "root_selector": null,
    "url_patterns": [
      "/search/results/all/"
    ]
  },
  "platform_id": "linkedin_com"
}