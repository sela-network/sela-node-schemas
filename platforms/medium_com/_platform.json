                  {
                    "type": "css",
                    "value": "time"
                  }
                ],
                "scope": "container"
              },
              "extraction": {
                "attribute": "datetime"
              },
              "required": false
            },
            {
              "name": "engagement_count",
              "field_type": "number",
              "selector": {
                "primary": {
                  "type": "css",
                  "value": "button[aria-label*='clap'] span"
                },
                "fallbacks": [
                  {
                    "type": "css",
                    "value": "[data-testid='clapCount']"
                  },
                  {
                    "type": "css",
                    "value": "button[data-action='show-post-reactions'] span"
                  }
                ],
                "scope": "container"
              },
              "extraction": "text",
              "transform": "parse_compact_number",
              "default": 0,
              "required": false
            }
          ],
          "actions": [
