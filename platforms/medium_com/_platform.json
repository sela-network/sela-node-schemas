      ],
      "root_selector": null,
      "content_schemas": [
        {
          "content_type": "article",
          "semantic_role": "article",
          "container_selector": {
            "primary": {
              "type": "css",
              "value": "article"
            },
            "fallbacks": [
              {
                "type": "css",
                "value": "[data-post-id]"
              },
              {
                "type": "css",
                "value": "div[role='listitem']"
              }
            ],
            "scope": "container"
          },
          "is_list": true,
          "fields": [
            {
              "name": "title",
              "field_type": "text",
              "selector": {
                "primary": {
                  "type": "css",
                  "value": "h2"
                },
                "fallbacks": [
                  {
                    "type": "css",
                    "value": "h3"
                  },
                  {
                    "type": "css",
                    "value": "[data-testid='postTitle']"
                  },
                  {
                    "type": "css",
                    "value": "a[href*='/@'] > div"
                  }
                ],
                "scope": "container"
              },
              "extraction": "text",
              "required": false
            },
            {
              "name": "link",
              "field_type": "url",
              "selector": {
                "primary": {
                  "type": "css",
                  "value": "h2 a"
                },
                "fallbacks": [
                  {
                    "type": "css",
                    "value": "h3 a"
                  },
                  {
                    "type": "css",
                    "value": "a[href*='/@'][href*='-']"
                  },
                  {
                    "type": "css",
                    "value": "a[href*='/p/']"
                  }
                ],
                "scope": "container"
              },
              "extraction": {
                "attribute": "href"
              },
              "required": false
            },
            {
              "name": "subtitle",
              "field_type": "text",
              "selector": {
                "primary": {
                  "type": "css",
                  "value": "h3"
                },
                "fallbacks": [
                  {
                    "type": "css",
                    "value": "p"
                  },
                  {
                    "type": "css",
                    "value": "[data-testid='postSubtitle']"
                  }
                ],
                "scope": "container"
              },
              "extraction": "text",
              "required": false
            },
            {
              "name": "author",
              "field_type": "text",
              "selector": {
                "primary": {
                  "type": "css",
                  "value": "a[href*='/@']:not([href*='-']) p"
                },
                "fallbacks": [
                  {
                    "type": "css",
                    "value": "[data-testid='authorName']"
                  },
                  {
                    "type": "css",
                    "value": "a[rel='author']"
                  }
                ],
                "scope": "container"
              },
              "extraction": "text",
              "required": false
            },
            {
              "name": "image",
              "field_type": "image",
              "selector": {
                "primary": {
                  "type": "css",
                  "value": "img[width]:not([width='20']):not([width='32']):not([width='40']):not([width='48'])"
                },
                "fallbacks": [
                  {
                    "type": "css",
                    "value": "picture img"
                  },
                  {
                    "type": "css",
                    "value": "img[loading='lazy']"
                  }
                ],
                "scope": "container"
              },
              "extraction": {
                "attribute": "src"
              },
              "required": false
            },
            {
              "name": "timestamp",
              "field_type": "timestamp",
              "selector": {
                "primary": {
                  "type": "css",
                  "value": "time[datetime]"
                },
                "fallbacks": [
                  {
                    "type": "css",
                    "value": "[datetime]"
