              "action_type": "click",
              "semantic_intent": "bookmark",
              "selector": {
                "primary": {
                  "type": "css",
                  "value": "button[aria-label*='save']"
                },
                "fallbacks": [
                  {
                    "type": "css",
                    "value": "button[aria-label*='bookmark']"
                  },
                  {
                    "type": "css",
                    "value": "button[data-action='save-post']"
                  }
                ],
                "scope": "container"
              },
              "requires_confirmation": false,
              "risk_level": 3
            }
          ],
          "nested_content": [],
          "required_fields": []
        }
