{
  "display_name": "Medium",
  "domains": [
    "medium.com",
    "www.medium.com",
    "m.medium.com"
  ],
  "page_type": {
    "content": {
      "actions": [
        {
          "intent": "like",
          "risk_level": 2,
          "selector": {
            "fallbacks": [
              "[aria-label*='like']",
              "[data-testid*='like']"
            ],
            "primary": "button[aria-label*='Like']"
          },
          "type": "click"
        },
        {
          "intent": "share",
          "risk_level": 3,
          "selector": {
            "fallbacks": [
              "[aria-label*='share']",
              "[data-testid*='share']"
            ],
            "primary": "button[aria-label*='Share']"
          },
          "type": "click"
        },
        {
          "intent": "comment",
          "risk_level": 4,
          "selector": {
            "fallbacks": [
              "[aria-label*='comment']",
              "[aria-label*='reply']"
            ],
            "primary": "button[aria-label*='Comment']"
          },
          "type": "click"
        }
      ],
      "container_selector": {
        "fallbacks": [
          "article[role='article']",
          "[data-urn]",
          "[role='listitem']"
        ],
        "primary": "[data-testid='post-preview']"
      },
      "content_type": "post",
      "fields": [
        {
          "extraction": "text",
          "field_type": "text",
          "name": "title",
          "selector": {
            "fallbacks": [
              "h3",
              "[role='heading']",
              "a[href*='/post'] span",
              "a[href*='/article'] span"
            ],
            "primary": "h2"
          }
        },
        {
          "extraction": {
            "attribute": "href"
          },
          "field_type": "url",
          "name": "link",
          "selector": {
            "fallbacks": [
              "h3 a",
              "a[href*='/post']",
              "a[href*='/article']",
              "a[href]:first-of-type"
            ],
            "primary": "h2 a"
          }
        },
        {
          "extraction": "text",
          "field_type": "text",
          "name": "subtitle",
          "selector": {
            "fallbacks": [
              "p",
              "[class*='excerpt']",
              "[class*='description']"
            ],
            "primary": "h3"
          }
        },
        {
          "extraction": {
            "attribute": "src"
          },
          "field_type": "image",
          "name": "image",
          "selector": {
            "fallbacks": [
              "picture img",
              "img[width]",
              "img"
            ],
            "primary": "img:not([class*='avatar']):not([class*='Avatar'])"
          }
        },
        {
          "extraction": "text",
          "field_type": "timestamp",
          "name": "timestamp",
          "selector": {
            "fallbacks": [
              "time[datetime]",
              "[data-time]",
              "[datetime]",
              "time"
            ],
            "primary": "div > span > div > div > a > div > span"
          }
        },
        {
          "default": 0,
          "extraction": "text",
          "field_type": "number",
          "name": "engagement_count",
          "selector": {
            "fallbacks": [
              "[aria-label*='like'] span",
              "button span"
            ],
            "primary": "[aria-label*='reaction'] span"
          },
          "transform": "parse_compact_number"
        }
      ],
      "is_list": true,
      "semantic_role": "article"
    },
    "display_name": "Search Results",
    "page_type_id": "search_results",
    "root_selector": null,
    "url_patterns": [
      "/search"
    ]
  },
  "platform_id": "medium.com"
}